{
  "name": "baltimore-food-drinks-map",
  "version": "0.1.0",
  "homepage": "https://barcrawl.live/",
  "private": true,
  "dependencies": {
    "@sentry/cli": "^2.32.1",
    "@sentry/react": "^8.7.0",
    "@supabase/supabase-js": "^2.43.4",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "leaflet": "^1.9.4",
    "lodash": "^4.17.21",
    "mapbox-gl": "^3.3.0",
    "randomcolor": "^0.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-grid-gallery": "^1.0.1",
    "react-leaflet": "^4.2.1",
    "react-map-gl": "^7.1.7",
    "react-scripts": "5.0.1",
    "react-spring-bottom-sheet": "^3.4.1",
    "styled-components": "^6.1.11",
    "uuid": "^9.0.1",
    "web-vitals": "^2.1.4",
    "yet-another-react-lightbox": "^3.19.0"
  },
  "scripts": {
    "predeploy": "npm run build",
    "start": "REACT_APP_HOMEPAGE=/ react-scripts start",
    "deploy": "REACT_APP_HOMEPAGE=https://barcrawl.live/ npm run build && echo 'barcrawl.live' > ./build/CNAME && gh-pages -d build",
    "oldbuild": "react-scripts build && npm run sentry:sourcemaps",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org cphsolutions --project javascript-react ./build && sentry-cli sourcemaps upload --org cphsolutions --project javascript-react ./build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "gh-pages": "^6.1.1"
  }
}
